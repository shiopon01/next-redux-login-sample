    "ts-node": "^8.8.1",
    "tslint": "^6.1.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^3.8.3",
    "typesync": "^0.6.1"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "/test/.*\\.test\\.(ts|js)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testEnvironment": "node"
